<div class="user_index">
  <div class="container">
    <div class="user-index">
      <ul class="user-list">
        <% @users.each do |user| %>
          <li><a href="/users/<%= user.id %>"><img src="<%= user.image %>"  class = "icon_image2">     <%= user.name %></a></li>
        <% end %>
      </ul>
    </div>
  </div>
</div>